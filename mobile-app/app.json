{
  "expo": {
    "name": "Oysterette",
    "slug": "oysterette",
    "version": "1.3.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "oysterette",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "url": "https://u.expo.dev/71f928a7-e51a-4136-8b2d-b1c3ff5dcda7"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "9",
      "infoPlist": {
        "NSCameraUsageDescription": "Oysterette needs access to your camera so you can take photos of your oysters for reviews.",
        "NSPhotoLibraryUsageDescription": "Oysterette needs access to your photo library so you can select photos of your oysters for reviews.",
        "NSPhotoLibraryAddUsageDescription": "Oysterette needs permission to save photos to your photo library."
      }
    },
    "android": {
      "package": "com.oysterette.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "versionCode": 9,
      "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "71f928a7-e51a-4136-8b2d-b1c3ff5dcda7"
      }
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.578059352307-osnf9gtai7o1g9h40bp0f997e286uit0"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Oysterette needs access to your photos to add images to your reviews.",
          "cameraPermission": "Oysterette needs access to your camera to take photos of your oysters."
        }
      ]
    ]
  }
}
