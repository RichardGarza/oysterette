{
  "name": "mobile-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "deploy-update": "./scripts/deploy-update.sh",
    "build:android": "eas build --platform android --profile preview",
    "update:preview": "eas update --branch preview",
    "update:production": "eas update --branch production",
    "depcheck": "depcheck --ignores='@types/*,typescript,tailwindcss,nativewind,expo-dev-client,expo-updates,react-native-safe-area-context,react-native-screens'",
    "prebuild:android": "npm run depcheck",
    "build:android:prod": "npm run prebuild:android && eas build --platform android --profile preview"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/slider": "5.0.1",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-native-ml-kit/text-recognition": "^2.0.0",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.1",
    "axios": "^1.13.1",
    "expo": "~54.0.20",
    "expo-apple-authentication": "^8.0.7",
    "expo-blur": "~15.0.7",
    "expo-camera": "^17.0.9",
    "expo-dev-client": "~6.0.17",
    "expo-haptics": "~15.0.7",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.12",
    "fuse.js": "^7.1.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.3.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "depcheck": "^1.4.7",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.2"
  },
  "private": true
}
